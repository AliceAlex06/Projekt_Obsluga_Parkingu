<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane fx:id="mainView" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Parking.Controller.WyborController">
    <children>
        <VBox style="-fx-alignment: center;">
            <children>
                <Button fx:id="wszystkieButton" maxWidth="160.0" minWidth="160.0" mnemonicParsing="false"
                        onAction="#wszystkieButton" prefWidth="160.0" text="Wszystkie samochody">
                    <VBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </VBox.margin>
                </Button>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Marka samochodu"/>
                <ComboBox fx:id="markaCombo" maxWidth="160.0" minWidth="160.0" prefWidth="160.0">
                    <VBox.margin>
                        <Insets bottom="20.0"/>
                    </VBox.margin>
                </ComboBox>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Numer rejestracyjny">
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </Text>
                <TextField fx:id="numerText" maxWidth="160.0" minWidth="160.0" prefWidth="160.0">
                    <VBox.margin>
                        <Insets bottom="20.0"/>
                    </VBox.margin>
                </TextField>
                <Button fx:id="szukajButton" mnemonicParsing="false" onAction="#szukajButton" text="Szukaj">
                    <VBox.margin>
                        <Insets bottom="20.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
